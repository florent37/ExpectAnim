<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.florent37.expectanim.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScrollActivity"/>
        <activity android:name=".SampleActivity"/>
        <activity android:name=".RotationActivity"/>
        <activity android:name=".SetNowActivity"/>
        <activity android:name=".FlipActivity"/>
        <activity android:name=".AlphaActivity" />
        <activity android:name=".ConcatActivity" />
    </application>

</manifest>